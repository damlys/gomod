{
  "configurations": [
    {
      "name": "Connect to localhost",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "host": "localhost",
      "port": 56268,
      "substitutePath": [
        {
          "from": "${workspaceFolder}",
          "to": "/workspace"
        }
      ]
    },
    {
      "name": "Launch current package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    }
  ]
}
